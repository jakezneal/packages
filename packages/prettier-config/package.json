{
    "name": "@jakezneal/eslint-config",
    "version": "0.1.0",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "unbuild",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "format": "prettier --write \"**/*.{ts,md,json}\"",
        "prepack": "pnpm build"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/node": "^22.4.1",
        "typescript": "^5.5.4",
        "unbuild": "^2.0.0"
    }
}
